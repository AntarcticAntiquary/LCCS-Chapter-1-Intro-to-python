n=input(':')
l=str(n)
m=list(l)
b=0
count=0
go=True
while go==True:
    for i in range(len(m)-1,-1,-1):
        if m[i]=='1':
            t=(2**count)
            count+=1
            b+=t
        elif m[i]!='0':
            print('Enter a valid value!')
            go=False
            break
    if go==True:
        print(b)
        go=False
